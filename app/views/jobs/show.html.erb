<div id="job">
		<h2><%= @job.name %></h2>
		<p><%= @job.location %></p>
		<p><%= @job.description %> </p>
		<p><%= @job.deadline %></p>
		<p><%= link_to 'Edit', edit_user_job_path(current_user,@job), class:'btn btn-default' %></p>
		<p><%= link_to 'Delete', user_job_path(current_user,@job), class:'btn btn-default' %></p>
		<%= link_to 'Back', jobs_path, :class => "btn btn-sm btn-default " %>
</div>